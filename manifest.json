{
  "name": "Course",
  "version": "1.0",
  "description": "Content Reader!",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "background": {
    "scripts": [
      "src/app.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "src/module/home/home.html",
    "options_page": "src/module/options/options.html",
    "default_icon": {
      "16": "src/assets/images/get_started16.png",
      "32": "src/assets/images/get_started32.png",
      "48": "src/assets/images/get_started48.png",
      "128": "src/assets/images/get_started128.png"
    }
  },
  "icons": {
    "16": "src/assets/images/get_started16.png",
    "32": "src/assets/images/get_started32.png",
    "48": "src/assets/images/get_started48.png",
    "128": "src/assets/images/get_started128.png"
  },
  "manifest_version": 2
}